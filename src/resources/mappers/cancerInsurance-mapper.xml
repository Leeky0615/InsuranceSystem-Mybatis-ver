<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Cancer">
<!--type : 사용될 클래스(mybatis-config.xml에서 alias해줌) -> ex) control.accidentReceipt.AccidentReceipt = AccidentReceipt  -->
<!--id : resultMap이름으로 사용될 id 설정  -->
    <resultMap type="Cancer" id="AccidentReceipt">
    <!--id : primary key로 사용되는 column / property : 해당 column의 값과 매치되는 (클래스에서 사용되는)변수이름  -->
        <result column="paymentdate" property="accidentReceiptName" />
        <result column="paymentmethod" property="customerName" />
        <result column="insurance_id" property="insuranceType"/>
        <result column="customer_id" property="customerId" />
    </resultMap>
 